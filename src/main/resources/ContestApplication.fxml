<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.collections.*?>
<?import javafx.event.*?>
<?import javafx.scene.input.*?>
<?import javafx.beans.property.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<SplitPane xmlns:fx="http://javafx.com/fxml"
	fx:controller="contest.ContestApplicationController" fx:id="splitPane"
	styleClass="root split-pane" stylesheets="@/filesComparator.css">
	<fx:define>
		<SimpleIntegerProperty fx:id="current" />
	</fx:define>
	<items>
		<ListView fx:id="allContests"
			onMouseClicked="#onMouseClickedListView1">
		</ListView>
		<ScrollPane fx:id="scrollPane2">
			<content>
				<VBox>
					<Text fx:id="text" wrappingWidth="500.0" />
					<VBox fx:id="images" />
				</VBox>
			</content>
		</ScrollPane>
		<ScrollPane fx:id="scrollPane3">
			<content>
				<VBox>
					<children>
						<Text fx:id="questionNumber"/>
						<Text fx:id="question" />
						<Button onAction="#onActionNext" text="Next" />
						<ListView fx:id="options"
							prefWidth="${splitPane.width*0.4}">
						</ListView>
					</children>
				</VBox>
			</content>
		</ScrollPane>
	</items>
</SplitPane>
