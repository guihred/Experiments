<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<?import javafx.scene.layout.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.control.cell.*?>
<?import javafx.scene.text.*?>
<?import javafx.event.*?>
<?import javafx.scene.input.*?>
<HBox xmlns:fx="http://javafx.com/fxml">
	<children>
		<VBox>
			<children>
				<Text text="Filter Results" />
				<TextField fx:id="resultsFilter" />
				<Text text="IPs" />
				<TextField fx:id="networkAddress" disable="true"/>
				<Button onAction="#onActionKibanaScan" text="Kibana Scan" />
				<Button onAction="#onActionClear" text="Clear" />
				<Button onAction="#onExportExcel" text="Export Excel" />
				<ProgressIndicator fx:id="progressIndicator"
					minHeight="40.0" minWidth="40.0" progress="0.0" />
			</children>
		</VBox>
		<TableView fx:id="commonTable" onKeyReleased="#copyContent" />
	</children>
</HBox>
